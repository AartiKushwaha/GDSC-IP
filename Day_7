https://leetcode.com/problems/linked-list-cycle-ii/

ListNode *detectCycle(ListNode *head) {    
      if(head==nullptr)
          return nullptr;
      ListNode* slow=head;
      ListNode* fast=head;

      while(fast!=nullptr && fast->next!=nullptr)
      {
          slow=slow->next;
          fast=fast->next->next;
          if(slow==fast)
          break;
      }
      if(fast!=nullptr && fast->next!=nullptr)
      {
          slow=head;
          while(slow!=fast)
          {
          slow=slow->next;
          fast=fast->next;
          }
          return slow;
      }

      return nullptr;
  }
  
